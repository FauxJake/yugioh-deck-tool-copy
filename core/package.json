{
    "name": "yugioh-deck-tool-api",
    "version": "1.0.0",
    "private": true,
    "description": "API for ygoprodeck.com",
    "module": "./dist/src/main.js",
    "types": "./dist/src/main.d.ts",
    "scripts": {
        "start": "npm run dist",
        "dist": "npm-run-all clean lint build",
        "clean": "rimraf ./dist ./spec/dist",
        "build": "tsc --build ./tsconfig.json",
        "watch": "tsc --build ./tsconfig.json -w",
        "lint": "eslint ./src/ ./spec/src/ --ext .ts",
        "fix": "eslint ./src/ ./spec/src/ --ext .ts --fix"
    },
    "bugs": {
        "url": "https://github.com/FelixRilling/yugioh-deck-tool/issues"
    },
    "homepage": "https://github.com/FelixRilling/yugioh-deck-tool#readme",
    "keywords": [
        "yugioh",
        "deck",
        "app"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/yugioh-deck-tool.git"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "lightdash": "^11.1.2",
        "lodash": "^4.17.15",
        "loglevel": "^1.6.7",
        "pako": "^1.0.8"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.149",
        "@types/pako": "^1.0.1",
        "@typescript-eslint/eslint-plugin": "^2.19.2",
        "@typescript-eslint/parser": "^2.19.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.2",
        "typescript": "^3.7.5"
    }
}
