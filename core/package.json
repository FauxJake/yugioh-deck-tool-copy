{
    "name": "yugioh-deck-tool-api",
    "version": "1.0.0",
    "private": true,
    "description": "API for ygoprodeck.com",
    "module": "./dist/esm/src/main.js",
    "types": "./dist/esm/src/main.d.ts",
    "scripts": {
        "start": "npm run dist",
        "dist": "npm-run-all clean lint build",
        "clean": "rimraf ./dist ./spec/dist",
        "build": "tsc --build ./tsconfig.json",
        "watch": "tsc --build ./tsconfig.json -w",
        "lint": "eslint ./src/ ./spec/src/ --ext .ts",
        "fix": "eslint ./src/ ./spec/src/ --ext .ts --fix",
        "test": "npm-run-all build test:jasmine",
        "test:jasmine": "jasmine --require=source-map-support/register"
    },
    "bugs": {
        "url": "https://github.com/FelixRilling/yugioh-deck-tool/issues"
    },
    "homepage": "https://github.com/FelixRilling/yugioh-deck-tool#readme",
    "keywords": [
        "yugioh",
        "deck",
        "app"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/yugioh-deck-tool.git"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "base64-js": "^1.3.1",
        "inversify": "^5.0.1",
        "lightdash": "^11.1.2",
        "lodash": "^4.17.15",
        "loglevel": "^1.6.7",
        "pako": "^1.0.8"
    },
    "peerDependencies": {
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@types/base64-js": "^1.2.5",
        "reflect-metadata": "^0.1.13",
        "@types/jasmine": "^3.5.7",
        "@types/lodash": "^4.14.149",
        "@types/pako": "^1.0.1",
        "@typescript-eslint/eslint-plugin": "^2.21.0",
        "@typescript-eslint/parser": "^2.21.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jasmine": "^3.5.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.16",
        "typescript": "^3.8.3"
    }
}
