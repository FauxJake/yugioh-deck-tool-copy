    "keywords": [
        "yugioh",
        "deck",
        "app"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/yugioh-deck-tool.git"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "base64-js": "^1.3.1",
        "inversify": "^5.0.1",
        "lightdash": "^12.0.0-1",
        "lodash": "^4.17.15",
        "loglevel": "^1.6.7",
        "pako": "^1.0.8",
        "url-parse": "^1.4.7"
    },
    "peerDependencies": {
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@types/base64-js": "^1.2.5",
        "@types/jasmine": "^3.5.10",
        "@types/lodash": "^4.14.149",
