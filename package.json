    },
    "homepage": "https://github.com/FelixRilling/yugioh-deck-tool#readme",
    "dependencies": {
        "bootstrap": "^4.1.0",
        "bootstrap-vue": "^2.0.0-rc.8",
        "lightdash": "^5.0.4",
