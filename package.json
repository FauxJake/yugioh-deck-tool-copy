{
    "name": "yugioh-deck-tool",
    "version": "v4.11.0",
    "description":
        "A tool to view deck prices, share and edit decks, create random decks and much more",
    "main": "./dist/app.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
        "build":
            "cross-env NODE_ENV=development webpack --progress --hide-modules",
        "dist":
            "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "lint": "npm-run-all lint:es lint:style",
        "lint:es": "eslint ./src/**/*.{js,vue}",
        "lint:style":
            "stylelint ./src/**/*.{scss,vue} --cache --cache-location=./.cache/stylelint/",
        "fix": "npm-run-all fix:es fix:style fix:prettier",
        "fix:es": "eslint ./src/**/*.{js,vue} --fix",
        "fix:style":
            "stylelint ./src/**/*.{scss,vue} --cache --cache-location=./.cache/stylelint/ --fix",
        "fix:prettier": "prettier ./src/**/*  --write"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/yugioh-deck-tool.git"
    },
    "keywords": ["yugioh", "deck", "app"],
    "author": "FelixRilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/yugioh-deck-tool/issues"
    },
    "homepage": "https://github.com/FelixRilling/yugioh-deck-tool#readme",
    "browserslist": ["> 1%", "last 5 versions", "not ie <=10"],
    "dependencies": {
        "bootstrap": "^4.1.1",
        "bootstrap-vue": "^2.0.0-rc.9",
        "lightdash": "^5.0.5",
        "pako": "^1.0.6",
        "vue": "^2.5.17-beta.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.46",
        "@babel/preset-env": "^7.0.0-beta.46",
        "babel-loader": "^8.0.0-beta.2",
        "cross-env": "^5.1.4",
        "css-loader": "^0.28.11",
        "eslint": "^4.19.1",
        "eslint-plugin-vue": "^4.5.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "node-sass": "^4.9.0",
        "npm-run-all": "^4.1.3",
        "sass-loader": "^7.0.1",
        "stylelint": "^9.2.0",
        "stylelint-config-recommended-scss": "^3.2.0",
        "stylelint-scss": "^3.1.0",
        "uglify-es": "^3.3.10",
        "uglifyjs-webpack-plugin": "^1.2.5",
        "vue-loader": "^14.2.2",
        "vue-template-compiler": "^2.5.17-beta.0",
        "webpack": "^4.8.0",
        "webpack-cli": "^2.1.3",
        "webpack-dev-server": "^3.1.4"
    }
}
