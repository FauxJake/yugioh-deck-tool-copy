        "lint": "eslint src/**/*.js src/**/*.vue",
        "fix": "npm-run-all fix:prettier fix:es",
        "fix:es": "eslint src/**/*.js src/**/*.vue --fix",
        "fix:prettier": "prettier src/**/*.js src/**/*.vue --write"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/yugioh-deck-tool.git"
