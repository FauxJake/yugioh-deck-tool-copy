{
    "name": "ygoprodeckprice",
    "version": "v4.0.0",
    "description": "ygodeckprice app for ygoprodeck.com",
    "main": "app/app.js",
    "constants": {
        "dirBase": {
            "input": "./src",
            "output": "./app"
        },
        "js": {
            "input": "app.js",
            "namespace": {
                "module": "myApp",
                "file": "app"
            },
            "targets": {
                "browsers": "chrome >= 58"
            }
        },
        "css": {
            "output": "app"
        }
    },
    "scripts": {
        "start": "npm-run-all lint build",
        "build": "npm-run-all dev",
        "dev": "node tasks/jsDev",
        "dist": "node tasks/jsDist",
        "lint": "eslint src/**/*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/ygodeckprice3.git"
    },
    "keywords": [
        "yugioh",
        "deck",
        "app"
    ],
    "author": "FelixRilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/ygodeckprice3/issues"
    },
    "homepage": "https://github.com/FelixRilling/ygodeckprice3#readme",
    "dependencies": {
        "bootstrap": "^4.0.0-beta",
        "clipboard-js": "^0.3.5",
        "file-saver": "^1.3.3",
        "lightdash": "^2.7.1",
        "vue": "^2.5.2",
        "vue-template-compiler": "^2.5.2"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "eslint": "^4.10.0",
        "eslint-config-vue": "^2.0.2",
        "eslint-plugin-vue": "^2.1.0",
        "npm-run-all": "^4.1.1",
        "rollup": "^0.50.0",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-commonjs": "^8.2.4",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-replace": "^2.0.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "rollup-plugin-vue": "^2.5.2",
        "uglify-es": "^3.1.6"
    }
}
